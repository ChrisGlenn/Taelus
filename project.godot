VI VII IX IV"
config/version="0.0.1"
run/main_scene="res://Scenes/Gameplay/SplashScreen/splash_screen.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/max_fps=120
config/icon="res://icon.png"

[autoload]

Globals="*res://Scripts/Globals.gd"
SaveLoad="*res://Scripts/SaveLoad.gd"
Events="*res://Scripts/Events.gd"
Clock="*res://Scenes/System/Clock/clock.tscn"
Functions="*res://Scripts/Functions.gd"
Dice="*res://Scripts/Dice.gd"
Chunks="*res://Scripts/Chunk/chunks.gd"
Items="*res://Scripts/Data/Items.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/mode=2
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/vsync/vsync_mode=2

[filesystem]

import/blender/enabled=false

[gui]

fonts/dynamic_fonts/use_oversampling=false
common/dynamic_fonts/use_oversampling=false

